{
  "env": {
    "browser": true,
    "jquery": true
  },
  "extends": ["airbnb-base", "plugin:prettier/recommended"],
  "globals": {
    "GM": true
  },
  "parser": "babel-eslint",
  "parserOptions": {
    "ecmaVersion": 2018,
    "sourceType": "module"
  },
  "plugins": ["prettier"],
  "rules": {
    "prettier/prettier": "error",
    "no-console": "off"
  }
}
